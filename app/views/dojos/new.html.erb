<div class="wrapper">
    <h1>New Dojo</h1>
    <form action="/dojos/create" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <p>Branch:<input type="text" name="branch"></input></p>
    <p>Street:<input type="text" name="street"></input></p>
    <p>City:<input type="text" name="city"></input></p>
    <p>State:<input type="text" name="state"></input></p>
    
    <button type="submit">Create</button>
    </form>
    <a href="/dojos/back">Go Back</a>    
</div>