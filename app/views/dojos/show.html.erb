<div class="wrapper">
<h2><%=@dojo.branch%></h2>
<h4>
<p>Address:  <%=@dojo.street%></p>
<p>City:  <%=@dojo.city%> State:  <%=@dojo.state%></p>
</h4>
<h2>Student Roster</h2>
<table>
    <thead>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Actions</th>
		</thead>
		<% @students.each do |s| %>
    <tbody>
			<td><%=s.first_name%></td>
			<td><%=s.last_name%></td>
			<td><%=s.email%></td>
			<td><a href="/dojos/<%=s.dojo_id%>/students/<%=s.id%>">Show |</a>
					<a href="/dojos/<%=s.dojo_id%>/students/edit/<%=s.id%>"> Edit |</a>
					<a href="/dojos/<%=s.dojo_id%>/students/destroy/<%=s.id%>">Destroy | </a>
			</td>
		</tbody>
		<% end %>
</table> 

<a href="/dojos/edit/<%=@dojo.id%>"> Edit |</a>
<a href="/dojos/home"> Home </a>
<a style="margin-left:300px" href="/dojos/<%=@dojo.id%>/students/new"> Add a Student</a>
<!-- end of wrapper -->
</div>